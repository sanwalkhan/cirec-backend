{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "prebuild": "apt-get update && apt-get install -y unixodbc-dev g++ make python3"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js" 
      }
    ]
  }