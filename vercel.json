{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": {
      "dist/index.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }