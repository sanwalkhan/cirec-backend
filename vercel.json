{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
              "prebuild": "apt-get update && apt-get install -y unixodbc-dev"
            }
          }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js" 
      }
    ]
  }
  